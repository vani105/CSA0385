#include <stdio.h>

int main() {
    int arr[] = {1, 2, 3, 4, 5};
    int size = sizeof(arr) / sizeof(arr[0]);
    printf("Original array:\n");
    for (int i = 0; i < size; i++) 
    {
        printf("%d\n", arr[i]);
    }
    for (int i = 0; i < size; i++) 
    {
        arr[3] = 5;
    }
    printf("Updated array:\n");
    for (int i = 0; i < size; i++) 
    {
        printf("%d ", arr[i]);
    }
    return 0;
}
